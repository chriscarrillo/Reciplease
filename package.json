{
  "name": "reciplease",
  "private": true,
  "scripts": {
    "start": "npm run build:prod",
    "clean": "trash dist/assets",
    "coverage": "npm run test -- --coverage",
    "coverage:ci": "cross-env JEST_JUNIT_OUTPUT=.coverage npm run coverage -- --ci --coverageReporters=cobertura --reporters=default --reporters=jest-junit",
    "coverage:watch": "npm run coverage -- --watch",
    "lint": "tsc && npm run lint:file -- .",
    "lint:file": "eslint --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint:fix:file -- .",
    "lint:fix:file": "npm run lint:file -- --fix",
    "test": "jest",
    "test:update": "npm run test -- -u",
    "test:watch": "npm run test -- --watch",
    "server": "npm run server:dev",
    "server:dev": "cross-env HOT_MODULES=true TS_NODE_PROJECT=tsconfig.base.json webpack-dev-server --port 3000",
    "server:story": "cross-env STORYBOOK=true TS_NODE_PROJECT=tsconfig.base.json start-storybook -c storybook -p 9001",
    "build:dev": "npm run clean && cross-env TS_NODE_PROJECT=tsconfig.base.json webpack",
    "build:prod": "cross-env NODE_ENV=production npm run build:dev",
    "build:story": "cross-env STORYBOOK=true NODE_ENV=production TS_NODE_PROJECT=tsconfig.base.json build-storybook -c storybook -o dist/story"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chriscarrillo/Reciplease.git"
  },
  "author": "Chris Carrillo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chriscarrillo/Reciplease/issues"
  },
  "homepage": "https://github.com/chriscarrillo/Reciplease#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@jupl/ts": "^0.2.6",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/bootstrap": "^4.3.2",
    "@types/dotenv-webpack": "^1.7.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "^16.9.6",
    "@types/react-hot-loader": "^4.1.1",
    "@types/storybook__addon-actions": "^5.2.1",
    "@types/storybook__addon-viewport": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.0.1",
    "@types/webpack": "^4.41.10",
    "@types/webpack-dev-server": "^3.10.1",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
